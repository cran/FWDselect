citHeader("To cite FWDselect in a publication use:")

citEntry(entry="Manual",
   title  = "{FWDselect}: {Selecting variables in regression models}",
   author = personList(as.person("Marta Sestelo"),
              as.person("Nora M. Villanueva"), as.person("Javier Roca-Pardi\\~{n}as")),
   year   = "{2013}",
   note   = "{R package version 1.1}",
    textVersion = 
   paste("Sestelo, M., Villanueva, N. M. and Roca-Pardi\\~{n}as, J. (2013).",
         "FWDselect: Selecting Variables in regression models")
)